<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.function.luo.threaddemo.MainActivity"
    android:orientation="vertical"
    >

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
       />

    <Button
        android:id="@+id/bt_fixed"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="FixedThreadPool"
        android:layout_margin="18dp"
        />

    <Button
        android:id="@+id/bt_cached"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="CachedThreadPool"
        android:layout_margin="18dp"
        />

    <Button
        android:id="@+id/bt_scheduled"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ScheduledThreadPool"
        android:layout_margin="18dp"
        />

    <Button
        android:id="@+id/bt_single"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="SingleThreadExecutor"
        android:layout_margin="18dp"
        />

    <Button
        android:id="@+id/bt_next"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="进入下一页"
        android:layout_margin="18dp"
        />
</LinearLayout>
